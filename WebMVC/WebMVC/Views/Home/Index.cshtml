@model  IEnumerable<WebMVC.Models.MockData>
@using WebMVC.Models;

@{
    ViewBag.Title = "Home Page";
}
<h1>Success</h1>
<div id="result" role="alert">
</div>
<nav>
    <div class="nav nav-tabs" id="nav-tab" role="tablist">
        <a class="nav-item nav-link active" id="nav-view-tab" data-toggle="tab" href="#nav-view" role="tab" aria-controls="nav-view" aria-selected="true">View List</a>
        <a class="nav-item nav-link" id="nav-new-tab" data-toggle="tab" href="#nav-new" role="tab" aria-controls="nav-new" aria-selected="false">Add New</a>
    </div>
</nav>

<div class="tab-content" id="nav-tabContent">
    <div class="tab-pane fade show active" id="nav-view" role="tabpanel" aria-labelledby="nav-view-tab">
        @Html.Partial("MockData", Model)
    </div>
    <div class="tab-pane fade show" id="nav-new" role="tabpanel" aria-labelledby="nav-new-tab">
        @Html.Action("AddOrEdit")
    </div>
</div>
</div>


